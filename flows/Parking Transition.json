    {
        "id": "6730a88e13a9e313",
        "type": "function",
        "z": "378f2327429a44b8",
        "name": "Update",
        "func": "msg.topic = \"vehicles\";\nvar vehicles = flow.get(\"vehicles\") || {};\nvar transition = flow.get(\"transition\") || 15;\n\nvar timestamp = new Date().getTime();\nvar list = [];\nvar changes = [];\n\nfor(var id in vehicles) {\n    if( vehicles[id].active ) {\n        if( vehicles[id].state === false && vehicles[id].age > transition )\n            vehicles[id].state = true;\n        vehicles[id].age = (timestamp - vehicles[id].birth) / 1000;\n        vehicles[id].lastupdate = timestamp;\n    } else {\n        if( vehicles[id].state && ((timestamp - vehicles[id].lastupdate) / 1000) > transition )\n            vehicles[id].state = false;\n    }\n    list.push(vehicles[id]);\n}\nlist.forEach(function(item){\n    if( item.state === false && item.active === false )\n        delete vehicles[item.id];\n})\n\nvar parkings = flow.get(\"parkings\") || [];\nparkings.forEach(function(parking) {\n    var changes = parking.state;\n    parking.state = false;\n    parking.active = false;\n    parking.age = 0;\n    list.forEach(function(vehicle) {\n        if( vehicle.cx > parking.x1 && vehicle.cx < parking.x2 && vehicle.cy > parking.y1 && vehicle.cy < parking.x2 ) {\n            if( parking.state !== vehicle.state )\n                changes = true;\n            else \n                changes = false;\n            parking.state = vehicle.state;\n            parking.active = vehicle.active;\n            parking.age = vehicle.age;\n            parking.update = vehicle.updated\n        } \n    });\n    if( changes )\n        changes.push(parking);\n})\n\nflow.set(\"parkings\",parkings);\n\nmsg.payload = changes;\n\nreturn msg;",
